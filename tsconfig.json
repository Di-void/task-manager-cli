{
  "display": "ESM",
  "_version": "18.1.0",
  "compilerOptions": {
    "lib": ["es2023"],
    "rootDir": "src",
    "outDir": "dist",
    "module": "ESNext",
    "target": "es2022",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "Bundler"
  }
}
